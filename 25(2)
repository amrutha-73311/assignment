def max_of_columns(matrix):
    rows = len(matrix)
    cols = len(matrix[0])
    column_max = [float('-inf')] * cols
    for row in matrix:
        for col, val in enumerate(row):
            column_max[col] = max(column_max[col], int(val))
    return column_max
n = int(input())  
matrix = []
for _ in range(n):
    row = input().split()
    matrix.append(row)
result = max_of_columns(matrix)
for val in result:
    print(val, end=" ")
