sno=int(input('enter a student number:'))
sname=input('enter student name:')
group=input('enter a group:')
s1=int(input('enter maths marks:'))
s2=int(input('enter computer marks:'))
s3=int(input('enter accounts marks:'))
tot=s1+s2+s3
avg=tot/3
if(avg>=90):
print('o grade')
elif(avg>=80):
print('A grade')
elif(avg>=70):
print('B grade')
elif(avg>=60):
print('C grade')
elif(avg>=50):
print('D grade')
else:
print('Pass')
