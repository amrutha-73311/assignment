def is_prime(num):
    if num < 2:
        return False
    for i in range(2, int(num**0.5) + 1):
        if num % i == 0:
            return False
    return True
def prime_count_up_to_n(n):
    count = 0
    for i in range(2, n + 1):
        if is_prime(i):
            count += 1
    return count
number = int(input("Enter a number: "))
result = prime_count_up_to_n(number)
print(f"The number of prime numbers up to {number} is: {result}")
