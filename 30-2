def is_perfect(num):
    divisors = []
    for i in range(1, num):
        if num % i == 0:
            divisors.append(i)
    return sum(divisors) == num

# Input: Number of test cases
t = int(input("Enter the number of test cases: "))

for _ in range(t):
    num = int(input("Enter a number: "))
    
    if is_perfect(num):
        print("PERFECT")
    else:
        print("NOT PERFECT")
